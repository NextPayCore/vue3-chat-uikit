{
  "name": "vue3-chat-uikit",
  "version": "0.0.0",
  "description": "A modern and flexible chat UI kit component for Vue 3",
  "keywords": [
    "vue",
    "vue3",
    "chat",
    "ui-kit",
    "typescript"
  ],
  "author": "tanmv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:NextPayCore/vue3-chat-uikit.git"
  },
  "private": false,
  "type": "module",
  "main": "dist/vue3-chat-uikit.umd.js",
  "module": "dist/vue3-chat-uikit.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/**/*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue3-chat-uikit.es.js",
      "require": "./dist/vue3-chat-uikit.umd.js"
    },
    "./style.css": "./dist/vue3-chat-uikit.css"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:types && vite build && npm run build:types",
    "build:types": "vue-tsc --build tsconfig.build.json",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "element-plus": "^2.11.4",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "~10.4.0",
    "jiti": "^2.5.1",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "^7.1.7",
    "vue": "^3.5.22",
    "vue-tsc": "^3.1.0"
  },
  "dependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/socket.io-client": "^1.4.36",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "socket.io-client": "^4.8.1",
    "vue3-google-login": "^2.0.34",
    "vuedraggable": "^2.24.3"
  },
  "volta": {
    "node": "22.20.0"
  }
}
